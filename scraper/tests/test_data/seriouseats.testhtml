    }, // loadNextEntry

    fsTOInit: function() {
      var obj = this;
      if(obj.loadTry) {
        obj.loadNextEntry();
      } else {
        setTimeout(function(){ obj.fsTOInit(); }, obj.checkTime);
      }
    },

    setListeners: function() {
      var obj = this;
      $(window).on('load resize', function(){
        obj.setHistoryThresholdLines();
      });
      $(window).on('scroll', function() {
        obj.loadTry = true;
        
        if(obj.historyTry) {
          return;
        }

        obj.historyTry = true;
        obj.hasEntryChanged();

        setTimeout(function() {
