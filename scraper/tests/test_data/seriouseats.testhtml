// SIDEBAR VIDEO PLAYER
var sidebarVids = [];

// Prototype constructor
function SidebarVid(el, playerObj, cookieName) {
  this._player = playerObj;
  this.$outerWrap = $(el);
  this.$videoEle = this.$outerWrap.find('video');
  this.linkEle = this.$outerWrap.find('.player-metadata');
  this.titleEle = this.$outerWrap.find('.title');

  this.currentVid = {
    index: 0,
    title: this.titleEle.text() || "",
    url: this.linkEle.attr('href')
  };
  // Init muted as false, will mute on load later
  this.muted = false;
  this.fullScreen = false;
  // autoplayInView is always true for now
  this.autoplayInView = true;
  this.userInteraction = false;
  // randomize determines if playlist should be randomized on init
  this.randomize = true;
  // randomized determines if playlist has already been randomized
  this.randomized = false;
  // pauseCheckTime is time to listen for valid canceling events before sending pause action to GA
  this.pauseCheckTime = 500;
  this.pauseTimeout = null;
  // preventPlayReport will preemptively cancel sending play actions to GA
  this.preventPlayReport = true;
  // resumePlayOnScroll remembers the play/pause state of the video as it scrolls in and out of view
  this.resumePlayOnScroll = true;
  // TODO: Can we init inView in a better way?
  this.inView = true;
  this.cookieName = cookieName;

  this.setPlayerListeners();
}

SidebarVid.prototype = {

  sendGAEvent: function(action, label) {
    seGlobalMethods.sendGAEvent("sidebar-video", action, label);
  }, // END sendGAEvent

  getCookie: function(cname) {
    return seGlobalMethods.getCookie(cname);
  }, // END getCookie

  setCookie: function(cname, cvalue, cdomain, exdays) {
    seGlobalMethods.setCookie(cname, cvalue, cdomain, exdays);
  }, // END setCookie

  cancelPauseReport: function(e) {
    var obj = this,
        eventType = e.type || e;
    if(obj.pauseTimeout !== null) {
      clearTimeout(obj.pauseTimeout);
      obj.pauseTimeout = null;
    }
  },

  setPlayerListeners: function() {
    var obj = this,
        objPlaylist = obj._player.playlist;

    // Not a listener, but we only need to initialize this once at the beginning
    objPlaylist.repeat(true);

    obj._player.on('loadstart', function(e) {

      // Just in case, try to cancel pause reporting here as well (in addition to those events below).
      obj.cancelPauseReport('loadstart');

      // Display the correct title and hyperlink
      var link_url = obj._player.mediainfo.link && obj._player.mediainfo.link.url;
      var link_text = obj._player.mediainfo.name;
      if(link_text && link_text.trim() != "") {
        if(link_url) {
          // Change/add href
          obj.linkEle.attr("href", link_url);
          obj.linkEle.attr("data-click-id", link_url);
          obj.currentVid.url = link_url;
        } else {
          // Remove href
          obj.linkEle.removeAttr("href");
          obj.linkEle.attr("data-click-id", link_text.trim());
          obj.currentVid.url = "";
        }
        obj.titleEle.text(link_text.trim());
        obj.currentVid.title = link_text.trim();
      }

      // Keep track of the last video played
      obj.currentVid.index = obj._player.playlist.currentItem();
      obj.setCookie(obj.cookieName, obj.currentVid.index);

    }); // END loadstart listener

    // Starting the player muted in the HTML, but leaving this in won't hurt
    obj._player.muted(true);
    obj.muted = true;

    // Add a flag if the user interacts with the player
    obj._player.on('click', function(evt) {
      obj.userInteraction = true;
    }); // END click listener
    // Adding mousedown and touchstart events since pause is triggered on these when video itself or the scrub bar is clicked
    obj._player.on('mousedown', function(evt) {
      obj.userInteraction = true;
    }); // END mousedown listener
    obj._player.on('touchstart', function(evt) {
      obj.userInteraction = true;
    }); // END touchstart listener

    // Track if the user mutes or unmutes the video
    obj._player.on('volumechange', function(evt) {
      if(obj.userInteraction === true) {
        if((obj._player.muted() && !obj.muted) || (!obj._player.muted() && obj.muted)) {
          obj.muted = obj._player.muted() ? true : false;

          // Pause any other videos on page if user unmutes
          // TODO: Extend this to video embeds (at least)
          if(!obj.muted) {
            $('video').each(function(){
              try {
                if(!($(this).is(obj.$videoEle))) {
                  this.pause();
                }
              } catch(e) {}
            });
          }
          var action = obj.muted ? "mute" : "unmute",
              label = obj.currentVid.title;
          obj.sendGAEvent(action, label);
        }
      }
    }); // END volumechange listener

    // Track if the user makes the video fullscreen or restores it to normal
    obj._player.on('fullscreenchange', function(evt) {
      if(obj.userInteraction === true) {
        obj.fullScreen = obj._player.isFullscreen() ? true : false;
        var action = obj.fullScreen ? "fullscreen" : "restore",
            label = obj.currentVid.title;
        obj.sendGAEvent(action, label);
      }
    }); // END fullscreenchange listener

    // Track if the user plays the video
    obj._player.on('play', function(evt) {
      obj.resumePlayOnScroll = true;
      if(obj.userInteraction === true && !obj.preventPlayReport) {
        var action = "play",
            label = obj.currentVid.title;
        obj.sendGAEvent(action, label);
      }
    }); // END play listener

    // Track if the user pauses the video
    obj._player.on('pause', function(evt) {
      if(obj.userInteraction === true && (obj.inView || obj.fullScreen)) {
        obj.preventPlayReport = false;
        obj.resumePlayOnScroll = false;
        var action = "pause",
            label = obj.currentVid.title;
        obj.pauseTimeout = setTimeout(function(){
          obj.sendGAEvent(action, label);
        }, obj.pauseCheckTime);
      }
    }); // END pause listener

    // Seeking event will cancel pause reporting (when progress bar is clicked).
    obj._player.on('seeking', function(evt) {
      obj.cancelPauseReport('seeking');
      // TODO: Seeking while paused prevents next user-initiated play event reporting
      obj.preventPlayReport = true;
    });

    // One of these events will cancel pause reporting (when video changes), or loadstart above.
    obj._player.on('ended', function(evt) { 
      obj.cancelPauseReport('ended');
      obj.preventPlayReport = true;
    });
    obj._player.on('abort', function(evt) {
      obj.cancelPauseReport('abort');
      obj.preventPlayReport = true;
    });
    obj._player.on('emptied', function(evt) {
      obj.cancelPauseReport('emptied');
      obj.preventPlayReport = true;
    });

    // Check if the  video is in view to play/pause appropriately
    setTimeout(obj.videoInViewCheck(), 1000);

  }, // END setPlayerListeners

  // If previously paused, remain paused when in view, else play when in view
  videoInViewCheck: function() {
    var obj = this;
    if(obj.autoplayInView === true) {
      var is_hidden = seGlobalState["documentHidden"] || false;
      var in_view = inViewportVert(obj.$outerWrap, obj.$outerWrap.outerHeight());
      var is_paused = obj._player.paused();

      obj.inView = (!is_hidden && in_view);

      if(obj.inView && is_paused && obj.resumePlayOnScroll) {
        obj.preventPlayReport = true;
        obj._player.play();
      } else if(!obj.inView && !is_paused && !obj.fullScreen) {
        obj._player.pause();
      }
    }
  } // END videoInViewCheck

}; // END SidebarVid.prototype

