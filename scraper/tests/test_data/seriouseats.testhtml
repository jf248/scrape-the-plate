          });
        }
      }

      var canCheckVid = false,
          loopVideoInViewCheckInt = null;

      function loopVideoInViewCheck() {
        if(seGlobalState && seGlobalState["sidebarVideo"]) {
          for(var i=0,j=sidebarVids.length;i<j;i++) {
            sidebarVids[i].videoInViewCheck();
          }
        } else {
          // Kills interval for in-view check
          clearInterval(loopVideoInViewCheckInt);
          loopVideoInViewCheckInt = null;
          // Kills document.hidden listener for in-view check
          if(removeListenerDocumentHidden && typeof removeListenerDocumentHidden === "function") { removeListenerDocumentHidden(loopVideoInViewCheck); }
        }
      }

      // currentY should be global variable from global JS
      if(typeof currentY !== "undefined") {
        // Debounce scroll check for video player in view
        loopVideoInViewCheckInt = setInterval(function() {
          if(canCheckVid) {
